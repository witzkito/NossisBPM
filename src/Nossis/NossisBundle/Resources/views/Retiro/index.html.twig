{% extends "NossisBundle::layout.html.twig" %}

{% block title %}Despachar{% endblock %}

{% block body %}
    <hr>
    
    <div class="container">
        
        <div class="form">
                <form action="{{ path('new_retiro') }}" method="post">
                {{ form_row(form.fechaSalida) }}
                {{ form_row(form.nroOrden) }}
                {{ form_row(form.empresa) }}
                {{ form_row(form.transportista) }}
                {{ form_row(form.patente) }}
                {{ form_row(form.formaCarga) }}
                <input class="submit" type="submit" value="Comenzar"  />
            
        </div>
           
    </div> 
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {{ form_javascript(form) }}
    <script>
        function setFocus()
        {
            document.getElementById("nossis_nossisbundle_retiro_codigo").focus();
        }
    
     </script>
{% endblock %}